<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="5dp"
    tools:context="${relativePackage}.${activityClass}" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:text="@string/label_civilite" />

        <Spinner
            android:id="@+id/Spinner1"
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            >
        </Spinner>
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:text="@string/label_prenom" />

        <EditText
            android:id="@+id/editTextJournalistePrenom"
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:ems="10" >
        </EditText>
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:text="@string/label_nom" />

        <EditText
            android:id="@+id/editTextJournalisteNom"
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:ems="10" >
        </EditText>
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:text="@string/label_email" />

        <EditText
            android:id="@+id/editTextJournalisteEmail"
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:ems="10" >
        </EditText>
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:text="@string/label_pseudo" />

        <EditText
            android:id="@+id/editTextJournalistePseudo"
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:ems="10" >
        </EditText>
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:text="@string/label_mdp" />

        <EditText
            android:id="@+id/editTextJournalisteMotDePasse"
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:ems="10" >
        </EditText>
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:text="@string/label_date_inscription" />

        <EditText
            android:id="@+id/editTextJournalisteDateInscription"
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:ems="10" >
        </EditText>
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:text="" />

        <CheckBox android:text="@string/label_offre_partenaire"
            android:id="@+id/CheckJournalisteOffre"
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:ems="10" >
        </CheckBox>
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:text="@string/label_photo" />

        <EditText
            android:id="@+id/editTextJournalistePhoto"
            android:layout_width="0dp"
            android:layout_weight="70"
            android:layout_height="wrap_content"
            android:ems="10" >
        </EditText>
    </LinearLayout>
    
    

</LinearLayout>
